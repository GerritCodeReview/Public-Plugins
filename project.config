[access]
	inheritFrom = Public-Projects
[project]
	description = Parent project for plugins/*
[access "refs/tags/*"]
	createTag = deny group Project Owners
	createTag = deny group gerrit-release-creators
	createSignedTag = deny group Project Owners
	createSignedTag = deny group gerrit-release-creators
[receive]
	requireContributorAgreement = true
	requireSignedOffBy = false
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/for/refs/meta/config"]
	push = deny group Registered Users
	pushMerge = deny group Registered Users
[allowSuperproject "gerrit"]
	matching = refs/heads/*:refs/heads/*
[access "refs/for/*"]
	deleteChanges = group Project Owners
